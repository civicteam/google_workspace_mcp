              "label": {
                "title": "Label",
                "type": "string"
              },
              "anchor": {
                "title": "Anchor",
                "type": "string"
              }
            },
            "required": [
              "label",
              "anchor"
            ],
            "title": "SearchEngineFacet",
            "type": "object"
          }
        },
        "title": "SearchEngineInfo"
      },
      "_meta": {
        "_fastmcp": {
          "tags": []
        }
      }
    },
    {
      "name": "search_custom_siterestrict",
