      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "search_engine_id": {
            "title": "Search Engine Id",
            "type": "string"
          },
          "title": {
            "title": "Title",
            "type": "string"
          },
          "total_indexed_results": {
            "default": null,
            "title": "Total Indexed Results",
            "type": "string"
          },
          "facets": {
